{
    "name": "kingga/gui",
    "description": "This is an extension to gabrielrcouto/php-gui which adds a MVC like framework onto it.",
    "require": {
        "gabrielrcouto/php-gui": "dev-master",
        "simple-crud/simple-crud": "^6.12",
        "sabre/xml": "^2.1",
        "symfony/console": "^4.2",
        "mustache/mustache": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Kingga\\Gui\\": "src/"
        },
        "files": [
            "src/Functions/Helpers.php"
        ]
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2"
    }
}
